<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:versionCode="1"
    android:versionName="1.1.0">
    <uses-feature android:name="android.hardware.location.gps"/>
    <uses-feature android:name="android.hardware.wifi"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="false"
        android:icon="@drawable/department"
        android:label="@string/app_name"
        android:roundIcon="@drawable/department"
        android:supportsRtl="false"
        android:theme="@style/Theme.Newappcpi"
        android:screenOrientation="portrait"
        android:dataExtractionRules="@xml/data_extraction_rules"
        tools:targetApi="s">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Newappcpi.NoActionBar"
            android:exported="true">

        </activity>
        <activity
            android:name=".Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".forms.StoreUser"/>
        <activity android:name=".ui.ambulance.Ambulance"/>
        <activity android:name=".GetDispos"/>
        <activity android:name=".FirebaseUIActivity"/>
        <activity
            android:exported="true"
            android:name=".forms.StoreConstantes"/>
        <service android:name=".Services.SetGardes"/>
        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".Services.SetPharma"/>
    </application>

</manifest>